{
  "name": "deeplinking-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec \"node --loader ts-node/esm src/server.ts\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "logs": "pm2 logs deeplink-resolver",
    "status": "pm2 status",
    "db:migrate": "npm run build && npx sequelize-cli db:migrate",
    "db:migrate:undo": "npm run build && npx sequelize-cli db:migrate:undo",
    "db:seed": "npm run build && npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harsh-aap/deeplink-resolver.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harsh-aap/deeplink-resolver/issues"
  },
  "homepage": "https://github.com/harsh-aap/deeplink-resolver#readme",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "pm2": "^6.0.14",
    "sequelize-cli": "^6.6.5",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}